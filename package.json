    "file-type": "^20.0.0",
    "gifuct-js": "^2.1.2",
    "jszip": "^3.10.1",
    "mathjs": "^14.0.1",
    "mime": "^4.0.6",
    "moment": "^2.30.1",
    "notiflix": "^3.2.8",
    "phaser": "^3.88.2",
    "phaser3-rex-plugins": "^1.80.12",
    "query-string": "^9.1.1",
    "send-intent": "^6.0.3",
    "stats-js": "^1.0.1",
    "wavesurfer.js": "^7.8.16"
  },
  "resolutions": {
    "cookie": ">=0.7.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry/cli",
      "esbuild",
      "sharp"
    ],
    "patchedDependencies": {
      "phaser@3.88.2": "patches/phaser@3.88.2.patch"
